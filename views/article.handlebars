<a href={{articlePageObject.articleLink}}>
    <h1>{{articlePageObject.articleName}}</h1>

</a>

<p>Author: {{articlePageObject.authorName}}</p>

<button class="goToHomePage">Back to main page</button>



<form>
    Text:
    <br />
    <input id="formInput" type="text" name="text" data_value={{articlePageObject.id}} value="write comment here" />
    <br />
    <input type="button" id="commentSubmitButton" value="Submit" />

</form>
<ul>
    <h4>Comments</h4>
    {{#each articlePageObject.array}}
    <li>
        <a>{{text}}</a>
    </li>
    {{/each}}
</ul>



<script src="../index.js"></script>